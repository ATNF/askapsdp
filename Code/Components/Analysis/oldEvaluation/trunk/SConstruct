# @file
# SConstruct build script for this module.
#
# @author Tim Cornwell tim.cornwell@csiro.au
#

# Always import this
from askapenv import env

env.Append(CCFLAGS=['-O2', '-g', '-DHAVE_LONG_LONG', '-DASKAP_DEBUG'])

# create build object with library name
pkg = env.AskapPackage("evaluation")
# add sub packages 
pkg.AddSubPackage("patternmatching")
pkg.AddSubPackage("evaluationutilities")

# run the build process
pkg()

