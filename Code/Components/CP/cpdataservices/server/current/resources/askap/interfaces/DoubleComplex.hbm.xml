<?xml version="1.0" encoding="WINDOWS-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="askap.interfaces">

    <class name="DoubleComplex" table="double_complex">
        <id type="long"  column="id">
            <generator class="native"/>
        </id>
        
        <!-- NOTE: The column name "real" cannot be used. It is a reserved word in MySql -->
        <property name="real" type="double" access="field" column="re" />
        
        <property name="imag" type="double" access="field" column="im"/>
        
    </class>

</hibernate-mapping>

