<?xml version="1.0" encoding="WINDOWS-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="askap.cp.calds.persist">

	<class name="GainSolutionElementBean" table="gain_solution_elements">
		<id type="long" column="id">
			<generator class="native" />
		</id>

		<property name="solutionID" type="long" column="solution_id" />

		<property name="antennaID" type="short" column="antenna_id" />
		<property name="beamID" type="short" column="beam_id" />

		<property name="g1Valid" type="boolean" column="g1_valid" />
		<property name="g1Real" type="double" column="g1_real" />
		<property name="g1Imag" type="double" column="g1_imag" />

		<property name="g2Valid" type="boolean" column="g2_valid" />
		<property name="g2Real" type="double" column="g2_real" />
		<property name="g2Imag" type="double" column="g2_imag" />

	</class>

</hibernate-mapping>
