<?xml version="1.0" encoding="WINDOWS-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="askap.cp.calds.persist">

	<class name="LeakageSolutionElementBean" table="leakage_solution_elements">
		<id type="long" column="id">
			<generator class="native" />
		</id>

		<property name="solutionID" type="long" column="solution_id" />

		<property name="antennaID" type="short" column="antenna_id" />
		<property name="beamID" type="short" column="beam_id" />

		<property name="leakageReal" type="double" column="leakage_real" />
		<property name="leakageImag" type="double" column="leakage_imag" />



	</class>

</hibernate-mapping>
