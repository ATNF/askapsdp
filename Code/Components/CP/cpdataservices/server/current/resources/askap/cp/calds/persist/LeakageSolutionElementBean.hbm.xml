<?xml version="1.0" encoding="WINDOWS-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="askap.cp.calds.persist">

	<class name="LeakageSolutionElementBean" table="leakage_solution_elements">
		<id type="long" column="id">
			<generator class="native" />
		</id>

		<property name="solutionID" type="long" column="solution_id" />

		<property name="antennaID" type="short" column="antenna_id" />
		<property name="beamID" type="short" column="beam_id" />

		<property name="d12Real" type="double" column="d12_real" />
		<property name="d12Imag" type="double" column="d12_imag" />

		<property name="d21Real" type="double" column="d21_real" />
		<property name="d21Imag" type="double" column="d21_imag" />

	</class>

</hibernate-mapping>
