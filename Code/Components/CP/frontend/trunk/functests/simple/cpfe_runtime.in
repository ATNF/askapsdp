askap.cp.frontend.runtime       = cpfe_runtime1

#
# Locator service config
#
askap.cp.frontend.ice.locator   =   IceGrid/Locator:tcp -h localhost -p 4061

#
# Adapter name for runtime
#
askap.cp.frontend.ice.adapter.name      =   cpfe_runtime_test
askap.cp.frontend.ice.adapter.endpoints =   tcp

#
# Network and protocol tracing
#
askap.cp.frontend.ice.trace.network     =   0
askap.cp.frontend.ice.trace.protocol    =   0

#
# Workflow configuration
#
askap.cp.frontend.workflow.activity0.runtime    =   cpfe_runtime1
askap.cp.frontend.workflow.activity0.type       =   SimpleMath
askap.cp.frontend.workflow.activity0.name       =   SimpleMathInstance0
askap.cp.frontend.workflow.activity0.in.port0   =   NumberStream0
askap.cp.frontend.workflow.activity0.in.port1   =   NumberStream1
askap.cp.frontend.workflow.activity0.out.port0  =   NumberStream2
askap.cp.frontend.workflow.activity0.custom.blah  = None

